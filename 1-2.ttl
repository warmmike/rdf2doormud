rsso:northwest a rdf:Property ;
          rdfs:subPropertyOf rsso:Direction .

rsso:southwest a rdf:Property ;
          rdfs:subPropertyOf rsso:Direction .

rsso:northeast a rdf:Property ;
          rdfs:subPropertyOf rsso:Direction .

rsso:southeast a rdf:Property ;
          rdfs:subPropertyOf rsso:Direction .

# Define individuals
